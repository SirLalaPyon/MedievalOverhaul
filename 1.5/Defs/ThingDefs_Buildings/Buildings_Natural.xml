	<ThingDef ParentName="BuildingNaturalBase" Name="DankPyon_HornetNestBase">
		<defName>DankPyon_HornetNest</defName>
		<label>hornet nest</label>
		<description>A hive of giant insects. If activated, it will spawn additional insects and hives over time, as well as valuable hornet honey.</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Natural/HornetNest</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>4,4</drawSize>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<rotatable>false</rotatable>
		<fillPercent>0.5</fillPercent>
		<statBases>
			<MaxHitPoints>230</MaxHitPoints>
			<Flammability>0.5</Flammability>
		</statBases>
		<pathCost>42</pathCost>
		<receivesSignals>true</receivesSignals>
		<blockWind>true</blockWind>
		<tickerType>Normal</tickerType>
		<passability>PassThroughOnly</passability>
		<killedLeavings>
			<DankPyon_HornetHoney>30</DankPyon_HornetHoney>
		</killedLeavings>
		<comps>
			<li Class="CompProperties_CanBeDormant">
				<startsDormant>false</startsDormant>
				<canWakeUpFogged>false</canWakeUpFogged>
			</li>
			<li Class="CompProperties_WakeUpDormant"/>
			<li Class="CompProperties_SelfhealHitpoints">
				<ticksPerHeal>6000</ticksPerHeal>
			</li>
			<li Class="CompProperties_Spawner">
				<thingToSpawn>DankPyon_HornetHoney</thingToSpawn>
				<spawnCount>2</spawnCount>
				<spawnIntervalRange>26000~30000</spawnIntervalRange>
				<spawnMaxAdjacent>20</spawnMaxAdjacent>
				<spawnForbidden>true</spawnForbidden>
				<saveKeysPrefix>hornethoney</saveKeysPrefix>
			</li>
			<li Class="CompProperties_Spawner">
				<thingToSpawn>DankPyon_HornetJelly</thingToSpawn>
				<spawnCount>10</spawnCount>
				<spawnIntervalRange>26000~30000</spawnIntervalRange>
				<spawnMaxAdjacent>13</spawnMaxAdjacent>
				<spawnForbidden>true</spawnForbidden>
				<saveKeysPrefix>hornetjelly</saveKeysPrefix>
			</li>
			<li Class="MedievalOverhaul.CompProperties_HornetNestSpawner">
				<spawnablePawnKinds>
					<li>DankPyon_Deathstinger</li>
				</spawnablePawnKinds>
				<spawnSound>Hive_Spawn</spawnSound>
				<initialPawnCount>0</initialPawnCount>
				<maxPawnCount>4</maxPawnCount>
				<pawnSpawnIntervalDays>0.5~1</pawnSpawnIntervalDays>
				<defendRadius>5</defendRadius>
				<faction>DankPyon_Hornets</faction>
			</li>
			<li Class="CompProperties_HeatPusher">
				<heatPerSecond>6</heatPerSecond>
				<heatPushMaxTemperature>38</heatPushMaxTemperature>
			</li>
			<li Class="CompProperties_SpawnerFilth">
				<filthDef>Filth_Slime</filthDef>
				<spawnCountOnSpawn>10</spawnCountOnSpawn>
				<spawnMtbHours>4</spawnMtbHours>
				<spawnRadius>5</spawnRadius>
			</li>
			<!--li Class="CompProperties_Maintainable">
				<ticksHealthy>60000</ticksHealthy>
				<ticksNeedsMaintenance>60000</ticksNeedsMaintenance>
				<damagePerTickRare>10</damagePerTickRare>
			</li-->
		</comps>
	</ThingDef>
	
	<!-- Golem Rock Generator -->
	<ThingDef ParentName="BuildingNaturalBase">
		<defName>DankPyon_GolemRock_Iron_MapGen</defName>
		<thingClass>MedievalOverhaul.Mineable_CompSpawnerDestroy</thingClass>
		<label>unusual iron formation</label>
		<description>An unusual iron formation, you hear something moving within.</description>
		<graphicData>
			<texPath>Natural/GolemRock</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<color>(110, 108, 106)</color>
			<drawSize>4,4</drawSize>
		</graphicData>
		<size>(2,2)</size>
		<altitudeLayer>Building</altitudeLayer>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>10000</MaxHitPoints>
			<Flammability>0</Flammability>
		</statBases>
		<pathCost>42</pathCost>
		<receivesSignals>true</receivesSignals>
		<blockWind>true</blockWind>
		<tickerType>Normal</tickerType>
		<passability>Impassable</passability>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>1</fillPercent>
		<coversFloor>true</coversFloor>
		<neverMultiSelect>true</neverMultiSelect>
		<saveCompressible>true</saveCompressible>
		<holdsRoof>true</holdsRoof>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<blockLight>true</blockLight>
		<fertility>0</fertility>
		<mineable>true</mineable>
		<building>
			<isResourceRock>true</isResourceRock>
			<mineableThing>DankPyon_IronOre</mineableThing>
			<mineableYield>450</mineableYield>
			<mineableScatterCommonality>0</mineableScatterCommonality>
			<mineableScatterLumpSizeRange>0~0</mineableScatterLumpSizeRange>
			<veinMineable>false</veinMineable>
			<isInert>true</isInert>
			<isNaturalRock>true</isNaturalRock>
			<canBuildNonEdificesUnder>false</canBuildNonEdificesUnder>
			<deconstructible>false</deconstructible>
			<destroyShakeAmount>0</destroyShakeAmount>
			<supportsWallAttachments>false</supportsWallAttachments>
		</building>
		<damageMultipliers>
		  <li>
			<damageDef>Thump</damageDef>
			<multiplier>4</multiplier>
		  </li>
		</damageMultipliers>
		<comps>
			<li Class="MedievalOverhaul.CompProperties_PawnSpawnerOnDestroy">
				<kindToSpawn>DankPyon_Golem_Iron</kindToSpawn>
				<chance>1</chance>
				<spawnAsPlayerFaction>false</spawnAsPlayerFaction>
				<faction>DankPyon_Forest_Faction</faction>
				<mentalStateDef>BerserkMechanoid</mentalStateDef>
				<numberToSpawn>1~1</numberToSpawn>
				<letterLabel>Golem Emerges</letterLabel>
				<letterDescription>A massive iron golem has eruprted from the ground. Make sure to take cover!</letterDescription>
				<letterDef>ThreatBig</letterDef>
				<disableCompDestroy>true</disableCompDestroy>
			</li>
		</comps>
	</ThingDef>

