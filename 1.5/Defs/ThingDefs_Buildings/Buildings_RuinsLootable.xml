	<!--======================= Lootable Shelf ==============================-->
	<!-- Alchemy Shelf -->
	<ThingDef ParentName="DankPyon_LootableShelf_Base">
		<defName>DankPyon_LootableShelf_AssortedHerbs</defName>
		<label>filled shelf (herbs)</label>
		<description>A shelf filled with herbs.</description>
		<graphicData>
			<texPath>Storage/Alchemy/Shelf_AssortedHerbs</texPath>
		</graphicData>

		<modExtensions>
			<li Class="MedievalOverhaul.BuildingLootableExtension">
				<enemysToSpawn>
					<li>Rat</li>
				</enemysToSpawn>
				<enemySpawnChance>0.10</enemySpawnChance>
				<hostileEnemy>false</hostileEnemy>
				<emptyGraphicData>
					<texPath>Buildings/Ruin/RuinedShelf</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(2.75,2.75)</drawSize>
				</emptyGraphicData>
				<searchSound>BuildingDestroyed_Wood_Medium</searchSound>
				<searchEffect>DustPuff</searchEffect>
				<effectSize>2.5</effectSize>
				<lootCount>0~10</lootCount>
				<lootChance>1</lootChance>
				<isRandom>true</isRandom>
				<randomItems>
					<li>DankPyon_Mindwort</li>
					<li>DankPyon_Poppy</li>
					<li>DankPyon_Fleawort</li>
					<li>DankPyon_FlyAgaric</li>
					<li>MedicineHerbal</li>
					<li>DankPyon_HerbalExtract</li>
				</randomItems>
			</li>
		</modExtensions>
	</ThingDef>

