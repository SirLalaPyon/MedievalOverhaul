			<MaxHitPoints>30</MaxHitPoints>
			<WorkToBuild>750</WorkToBuild>
			<Mass>5</Mass>
			<Flammability>1.0</Flammability>
			<Cleanliness>-5</Cleanliness>
		</statBases>
		<tickerType>Normal</tickerType>
		<size>(1,1)</size>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
		</building>
		<costList>
			<DankPyon_ComponentBasic>1</DankPyon_ComponentBasic>
		</costList>
		<stuffCategories>
			<li>Metallic</li>
			<li>Stony</li>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>30</costStuffCount>
		<comps>
			<li Class="MedievalOverhaul.CompProperties_RefuelableCustom">
				<fuelConsumptionRate>2.2</fuelConsumptionRate>
				<fuelCapacity>10.0</fuelCapacity>
				<fuelFilter>
					<categories>
						<li>FoodRaw</li>
					</categories>
					<thingDefs>
						<li>Hay</li>
						<li>Kibble</li>
					</thingDefs>
				</fuelFilter>
				<defaultIngredientFilter>
					<categories>
						<li>FoodRaw</li>
					</categories>
					<thingDefs>
						<li>Hay</li>
						<li>Kibble</li>
					</thingDefs>
					<disallowedCategories>
						<li>EggsFertilized</li>
					</disallowedCategories>
					<disallowedThingDefs>
						<li>InsectJelly</li>
					</disallowedThingDefs>
				</defaultIngredientFilter>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
				<fuelIconPath>UI/EmptyMeat_Icon</fuelIconPath>
			</li>
			<li Class="CompProperties_Facility">
				<maxSimultaneous>1</maxSimultaneous>
			</li>
		</comps>
		<inspectorTabs>
			<li>MedievalOverhaul.ITab_FuelCustom</li>
		</inspectorTabs>
