<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Weapons Expanded</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <!--Recipes-->
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="VWE_MeleeWeapon_Axe"]/recipeMaker/recipeUsers</xpath>
                    <value>
                        <li>DankPyon_Workbench</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="VWE_Tool_Whip"]/recipeMaker/recipeUsers</xpath>
                    <value>
                        <li>DankPyon_Workbench</li>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>