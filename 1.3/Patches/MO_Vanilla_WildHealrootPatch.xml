<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods>
		<match Class="PatchOperationSequence">
		<success>Always</success>
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/BiomeDef[defName="DankPyon_DarkForest"]/wildPlants/Plant_Healroot</xpath>
					<value>
						<Plant_HealrootWild>0.08</Plant_HealrootWild>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

