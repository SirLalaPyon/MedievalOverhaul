            <DankPyon_KnightAmboise>25</DankPyon_KnightAmboise>
            <DankPyon_Medieval_Lord>60</DankPyon_Medieval_Lord>
          </options>
        </li>
      </pawnGroupMakers>
  </FactionDef>

